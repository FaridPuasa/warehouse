<!DOCTYPE html>
<html lang="en">
<head>

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/public/style.css">
    <title>Login</title>
</head>
<body>

  <ul>
    <li><a href="/">Zalora Main</a></li>
    <li><a href="/itemin">Zalora In</a></li>
    <li><a href="/itemout">Zalora POD</a></li>
    <li><a href="/reentry">Re-Entry</a></li>
    <li><a href="/pharmacyin">Pharmacy Inventory</a></li>
    <li><a href="/pharmacyout">Pharmacy POD</a></li>
    <li><a href="/grpmy">GRP Malaysia</a></li>
    <li><a href="/dispatch">Dispatcher Report</a></li>
    <li><a href="/central">General Tracker</a></li>
  </ul>

<table>
  <thead>
    <tr>
      <th scope = "col">Dispatch Name</th>
      <th scope = "col">Car Rregistration</th>
      <th scope = "col">Date</th>
      <th scope = "col">Amount Collected</th>
      <th scope = "col">Parcel Given</th>
      <th scope = "col">Success Delivery</th>
      <th scope = "col">Failed Delivery</th>
      <th scope = "col">Cancelled Delivery</th>
      <th scope = "col">Change to Self Collect</th>
      <th scope = "col">Re-Schedule</th>
      <th scope = "col">Pickup</th>
      <th scope = "col">Parcel Pickup</th>
      <th scope = "col">Return</th>
      <th scope = "col">Return Parcel</th>
    </tr>
  </thead>


    <% dispatch.forEach(dispatch => {%>
        <tbody>
          <tr class="content">
            <th scope="row"><%= dispatch.name %></th>
            <td><%= dispatch.carNumber %></td>
            <td><%= dispatch.dateSubmit %></td>
            <td><%= dispatch.totalCollection %></td>
            <td><%= dispatch.given %></td>
            <td><%= dispatch.success %></td>
            <td><%= dispatch.failed %></td>
            <td><%= dispatch.cancel %></td>
            <td><%= dispatch.selfCollect %></td>
            <td><%= dispatch.reSchedule %></td>
            <td><%= dispatch.pickup %></td>
            <td><%= dispatch.pickupVal %></td>
            <td><%= dispatch.return %></td>
            <td><%= dispatch.returnVal %></td>
          </tr>
        </tbody>
    <%})%>
  </table>

   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</body>
</html>

